<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>Servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>uploadExcelServlet</servlet-name>
    <servlet-class>Servlet.UploadExcelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadExcelServlet</servlet-name>
    <url-pattern>/uploadExcelServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>viewData</servlet-name>
    <servlet-class>Servlet.ViewDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewData</servlet-name>
    <url-pattern>/viewData</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddRecordServlet</servlet-name>
    <servlet-class>Servlet.AddRecordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddRecordServlet</servlet-name>
    <url-pattern>/AddRecordServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ExportDataServlet</servlet-name>
    <servlet-class>Servlet.ExportDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExportDataServlet</servlet-name>
    <url-pattern>/ExportDataServlet</url-pattern>
  </servlet-mapping>
</web-app>
